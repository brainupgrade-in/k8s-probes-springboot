{
  "name": "Spring Boot Dev Container",
  "image": "ghcr.io/devcontainers/java:17",
  "features": {
    "ghcr.io/devcontainers/features/maven:1": {}
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "vscjava.vscode-maven",
        "vscjava.vscode-spring-boot",
        "redhat.java"
      ]
    }
  },
  "remoteUser": "vscode"
}